    {
      "parameters": {
        "jsCode": "const texto = $json.numero;\n\n// Regex que captura todas as variações de \"nome\" e \"contato\"\nconst regex = /(n[úu]mero|numero|nome|n)\\s*:\\s*([^:\\n]+)[\\s\\S]*?(telefone|t[ée]lefone|t|contato)\\s*:\\s*([+\\d\\s-]+)/gi;\n\nconst resultados = [];\nlet match;\n\nwhile ((match = regex.exec(texto)) !== null) {\n  let nome = match[2].trim();\n  let contato = match[4].trim();\n\n  // Remove tudo que não for número\n  contato = contato.replace(/\\D/g, '');\n\n  // Adiciona o 55 se não tiver\n  if (!contato.startsWith('55')) {\n    contato = '55' + contato;\n  }\n\n  resultados.push({\n    nome,\n    contato\n  });\n}\n\n// Caso não encontre nenhum, retorna item vazio pra não quebrar fluxo\nif (resultados.length === 0) {\n  return [{ json: { nome: null, contato: null } }];\n}\n\n// Retorna cada par nome/contato como item separado\nreturn resultados.map(r => ({ json: r }));\n"
      },
